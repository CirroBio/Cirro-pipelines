{
  "json.schemas": [
    {
      "fileMatch": [
        "reference/references.json"
      ],
      "url": "./schemas/references.schema.json"
    },
    {
      "fileMatch": [
        "**/process-input.json"
      ],
      "url": "./schemas/process-input.schema.json"
    },
    {
      "fileMatch": [
        "**/process-definition.json"
      ],
      "url": "./schemas/process-definition.schema.json"
    },
    {
      "fileMatch": [
        "**/process-form.json"
      ],
      "url": "./schemas/process-form.schema.json"
    }
  ],
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./process",
    "-p",
    "*test*.py"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true
}
